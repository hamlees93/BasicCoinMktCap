<p id="notice"><%= notice %></p>

<h1>Portfolios</h1>

<%= render partial: "table", object: @portfolios %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @portfolios.each do |k, v| %>
      <tr>
        <td><%= link_to 'Destroy', v, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= render 'new_coin', portfolio: @portfolio %>

<%= link_to "Log Out", destroy_user_session_path, method: :delete %>
